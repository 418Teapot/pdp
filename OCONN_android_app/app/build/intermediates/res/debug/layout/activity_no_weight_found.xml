<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="dk.http418.oconn.NoWeightFound">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Ingen vægt fundet!"
        android:id="@+id/noWtxt"
        android:layout_above="@+id/noWtxt2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="91dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="Den ønskede vægt blev ikke fundet, prøv igen - og hvis problemet fortsætter - kontakt pakkevagten!"
        android:id="@+id/noWtxt2"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Prøv igen"
        android:id="@+id/noW_retry_closeBtn"
        android:layout_below="@+id/noWtxt2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="67dp" />
</RelativeLayout>
